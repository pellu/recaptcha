{
    "name": "pellu/recaptcha",
    "description": "Laravel package for Google Recaptcha v3",
    "keywords": [
        "recaptcha",
        "recaptcha v3",
        "google recaptcha",
        "no captcha",
        "laravel",
        "laravel 5",
        "laravel 6",
        "laravel 7",
        "laravel 8",
        "laravel 9",
        "laravel 10",        
        "packalyst",
        "pellu"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/pellu/recaptcha/issues",
        "source": "https://github.com/pellu/recaptcha",
        "docs": "https://github.com/pellu/recaptcha/blob/master/README.md"
    },
    "authors": [
        {
            "name": "mostafaznv",
            "email": "mostafa.zeinivand@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || 6.* || 7.* || 9.* || 10.*",
        "guzzlehttp/guzzle": "6.* || 7.*"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Pellu\\Recaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pellu\\Recaptcha\\RecaptchaServiceProvider"
            ],
            "aliases": {
                "Recaptcha": "Pellu\\Recaptcha\\Facades\\Recaptcha"
            }
        }
    }
}
